{
  "extends": "next/core-web-vitals",
  "plugins": [
    "prettier"
  ],
  "globals": {
    "__DEV__": true
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "useTabs": true,
        "bracketSameLine": true,
        "endOfLine": "auto",
        "tabWidth": 4,
        "trailingComma": "es5",
        "semi": false,
        "singleQuote": false,
        "printWidth": 100
      }
    ],
    "no-console": "warn",
    "no-unused-vars": "error",
    "no-constant-binary-expression": "warn",
    "no-new-native-nonconstructor": "warn",
    "no-self-compare": "warn",
    "no-duplicate-imports": "warn"
  }
}